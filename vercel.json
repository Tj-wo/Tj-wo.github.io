{
  "version": 2,
  "builds": [
    {
      "src": "my-portfolio/portfolio-backend/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "my-portfolio/portfolio-frontend/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "my-portfolio/portfolio-backend/server.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}
